{
  "name": "Progressive Web App",
  "short_name": "Demo PWA",
  "description": "Demo app for progressive web apps",
  "start_url": "./index.html?utm=homescreen",
  "display": "standalone",
  "orientation": "portrait",
  "background_color": "#f5f5f5",
  "theme_color": "#29434d",
  "protocol_handlers": [
    {
      "scheme": "web+github",
      "url": "https://fabiorocha.github.io/pwa/?profile=%s",
      "title": "Github Profile Card",
      "icon": {
        "src": "images/catalogicon-44.png",
        "type": "image/png",
        "sizes": "44x44"
      }
    }
  ],
  "file_handlers": [
    {
      "action": "https://microsoft.com",
      "accept": {
        "text/csv": "Not a valid extension"
      }
    },
    {
      "action": "/",
      "accept": {
        "text/csv": [ ".txt2, txt3" ],
        "application/vnd.alternative-graph-app.graph": ".graph2"
      }
    }
  ],
  "icons": [
    {
      "src": "./images/icons/android-chrome-192x192.png",
      "type": "image/png",
      "sizes": "192x192"
    },
    {
      "src": "./images/icons/android-chrome-512x512.png",
      "type": "image/png",
      "sizes": "512x512"
    },
    {
      "src": "./images/icons/android-maskable-192x192.png",
      "type": "image/png",
      "sizes": "192x192",
      "purpose": "maskable"
    },
    {
      "src": "./images/icons/android-maskable-512x512.png",
      "type": "image/png",
      "sizes": "512x512",
      "purpose": "maskable"
    }
  ],
  "gcm_sender_id": "847244712742",
  "gcm_user_visible_only": true
}
